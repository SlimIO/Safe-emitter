{
  "name": "@slimio/safe-emitter",
  "version": "1.0.5",
  "description": "Safe and isolated Event Emitter with Asynchronous capabilities",
  "main": "index.js",
  "scripts": {
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/safeEmitter.git"
  },
  "keywords": [],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/safeEmitter/issues"
  },
  "homepage": "https://github.com/SlimIO/safeEmitter#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@escommunity/minami": "^1.0.0",
    "@types/node": "^10.12.18",
    "eslint": "^5.10.0",
    "husky": "^1.3.0",
    "jsdoc": "^3.5.5",
    "nyc": "^13.0.1"
  },
  "dependencies": {
    "@slimio/eslint-config": "^2.0.3",
    "ava": "^1.0.1",
    "execa": "^1.0.0",
    "inquirer": "^6.2.1",
    "kleur": "^3.0.1"
  }
}
